    .thread T_2;
    .scope S_00000190278a0ca0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000019027907950_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_00000190278a0ca0;
T_4 ;
    %delay 5000, 0;
    %load/vec4 v0000019027907950_0;
    %inv;
    %store/vec4 v0000019027907950_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_00000190278a0ca0;
T_5 ;
    %vpi_call 2 60 "$dumpfile", "sim/xor_cipher.vcd" {0 0 0};
    %vpi_call 2 61 "$dumpvars", 32'sb00000000000000000000000000000000, S_00000190278a0ca0 {0 0 0};
    %vpi_call 2 62 "$display", "\012--- XOR Cipher Simulation Started ---\012" {0 0 0};
    %pushi/vec4 179, 0, 8;
    %store/vec4 v0000019027909110_0, 0, 8;
    %pushi/vec4 202, 0, 8;
    %store/vec4 v0000019027907f90_0, 0, 8;
    %fork TD_tb_xor_cipher.run_test, S_00000190278a12d0;
    %join;
    %pushi/vec4 172, 0, 8;
    %store/vec4 v0000019027909110_0, 0, 8;
    %pushi/vec4 65, 0, 8;
    %store/vec4 v0000019027907f90_0, 0, 8;
    %fork TD_tb_xor_cipher.run_test, S_00000190278a12d0;
    %join;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0000019027909110_0, 0, 8;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0000019027907f90_0, 0, 8;
    %fork TD_tb_xor_cipher.run_test, S_00000190278a12d0;
    %join;
    %pushi/vec4 179, 0, 8;
    %store/vec4 v0000019027909110_0, 0, 8;
    %pushi/vec4 79, 0, 8;
    %store/vec4 v0000019027907f90_0, 0, 8;
