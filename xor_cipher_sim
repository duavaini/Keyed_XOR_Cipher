TD_tb_xor_cipher.run_test ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0000019027908850_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000019027908c10_0, 0, 1;
    %load/vec4 v0000019027909110_0;
    %store/vec4 v0000019027909390_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0000019027909750_0, 0, 8;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000019027908850_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0000019027908c10_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000019027908c10_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0000019027907f90_0;
    %store/vec4 v0000019027909750_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 53 "$display", "KEY=%h  PLAINTEXT=%h  -->  CIPHERTEXT=%h  DECRYPTED=%h", v0000019027909390_0, v0000019027909750_0, v0000019027909250_0, v0000019027908990_0 {0 0 0};
    %delay 10000, 0;
    %end;
    .scope S_00000190278567b0;
T_1 ;
    %wait E_0000019027898ee0;
    %load/vec4 v0000019027909070_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1, 0, 8;
    %assign/vec4 v00000190279096b0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0000019027909570_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0000019027907e50_0;
    %cmpi/e 0, 0, 8;
    %flag_mov 8, 4;
    %jmp/0 T_1.4, 8;
    %pushi/vec4 1, 0, 8;
    %jmp/1 T_1.5, 8;
T_1.4 ; End of true expr.
    %load/vec4 v0000019027907e50_0;
    %jmp/0 T_1.5, 8;
 ; End of false expr.
    %blend;
T_1.5;
    %assign/vec4 v00000190279096b0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v00000190279096b0_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0000019027908cb0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v00000190279096b0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_000001902785e590;
T_2 ;
    %wait E_0000019027898ee0;
    %load/vec4 v000001902789fca0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 1, 0, 8;
    %assign/vec4 v000001902789f980_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v000001902789fd40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %load/vec4 v000001902789f8e0_0;
    %cmpi/e 0, 0, 8;
    %flag_mov 8, 4;
    %jmp/0 T_2.4, 8;
    %pushi/vec4 1, 0, 8;
    %jmp/1 T_2.5, 8;
T_2.4 ; End of true expr.
