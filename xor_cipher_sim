    %jmp/0 T_2.5, 8;
 ; End of false expr.
    %blend;
T_2.5;
    %assign/vec4 v000001902789f980_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v000001902789f980_0;
    %parti/s 7, 0, 2;
    %load/vec4 v000001902789fa20_0;
